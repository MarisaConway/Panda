<div class ="jumbotron">
    <h1>Welcome to Pandas!</h1>
</div>
<a href="/new"><button class="btn btn-info">New Panda</button></a>
<table class="table table-striped table-hover">
    <tr>
        <th>Type</th>
        <th>Location</th>
        <th>Cuteness</th>
        <th>Weight</th>
        <th>Description</th>
        <th>Created At</th>
    </tr>
    @foreach(var panda in ViewBag.Pandas)
    {
        <tr>
            <td><a href="/panda/@panda.PandaId">@panda.Type</a></td>
            <td>@panda.Location</td>
            <td>@panda.Cuteness</td>
            <td>@panda.Weight</td>
            <td>@panda.Description</td>
            <td>@panda.Created_At</td>
        </tr>
    }

</table>
