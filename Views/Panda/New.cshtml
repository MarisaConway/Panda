<h1>Add a Panda</h1>
<form action ="/create" method ="post">
    <h5>Type</h5><input type="text" name="Type" class="form-control" />
    <h5>Location</h5><input type="text" name="Location"class="form-control" />
    <h5>Cuteness</h5><select class="form-control" name="Cuteness">
        @for(int i=1; i<6; i++)
        {
            <option>@i</option>
        }
    </select> 
    <h5>Weight</h5><input type="number" name="Weight" class="form-control" min="10" />
    <h5>Description</h5><textarea name ="Description" placeholder="Description" class="form-control"> </textarea>
    <input type="submit" class="btn btn-success" />
</form>